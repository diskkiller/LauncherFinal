<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_private_market">

    <com.zhongti.huacailauncher.widget.bamUI.BamFrameLayout
        android:id="@+id/fl_private_market_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:clickable="true"
        android:padding="9px">

        <ImageView
            android:layout_width="33px"
            android:layout_height="33px"
            android:src="@drawable/img_chacha" />
    </com.zhongti.huacailauncher.widget.bamUI.BamFrameLayout>

    <com.zhongti.huacailauncher.widget.bamUI.BamButton
        android:id="@+id/btn_private_market_go"
        android:layout_width="264px"
        android:layout_height="74px"
        android:layout_marginLeft="125px"
        android:layout_marginTop="625px"
        android:background="@drawable/img_btn_go_market"
        android:clickable="true"
        android:focusable="true" />

    <ImageView
        android:id="@+id/iv_private_market_er"
        android:layout_width="260px"
        android:layout_height="260px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="200px"
        android:layout_marginTop="320px"
        tools:src="@drawable/img_er_market_or_release" />
</com.zhy.autolayout.AutoRelativeLayout>